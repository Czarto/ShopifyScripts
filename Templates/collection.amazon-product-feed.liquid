    {%- for product in collection.products -%}
        {%- liquid
        assign item_name = product.title | replace: ',','' | remove: '"' | remove: "'" | remove: "," |  strip_html | strip
        assign brand_name = product.vendor
        assign product_description = product.description | strip_html | strip_newlines | escape | remove: "," | truncate: 2000
        if product.metafields.global.description_tag.size > 0
            assign product_description = product.metafields.global.description_tag | remove: ","
        endif

        assign footwear_age_group = 'adult'
        assign age_range_description = footwear_age_group
        -%}
        {%- for variant in product.variants -%}
            {%- liquid
            assign item_sku = variant.sku
            assign external_product_id = variant.barcode
            assign footwear_width = 'Medium'
            assign footwear_size = ''
            assign color_name = ''
            assign color_map = ''
            assign sale_price = variant.price | money_without_currency
            assign standard_price = variant.price | money_without_currency
            assign list_price = variant.compare_at_price | money_without_currency
            assign quantity_available = 0
            assign item_weight_unit_of_measure = variant.weight_unit
            assign item_weight = variant.weight_in_unit
            assign condition_type = 'New'

            for option in product.options
                if option == 'Color' or option == 'Colour'
                    assign color_name = variant.options[forloop.index0]
                elsif option == 'Size'
                    assign footwear_size = variant.options[forloop.index0]
                endif
            endfor

            assign footwear_gender = 'unisex'
            assign target_gender = 'unisex'
            if footwear_size contains 'Ladies'
                assign footwear_gender = 'Women'
                assign target_gender = 'Female'
            elsif footwear_size contains 'Mens'
                assign footwear_gender = 'Men'
                assign target_gender = 'Male'
            endif

            # Product Type
            assign feed_product_type = 'none'
            case product.type
                when 'Mukluk'
                    assign feed_product_type = 'boot'
                when 'Slipper'
                    assign feed_product_type = 'slipper'
                when 'Moccasin'
                    assign feed_product_type = 'shoe'
            endcase
            case item_name
                when 'Tipi'
                    assign feed_product_type = 'slipper'
            endcase

            # Recommended Browse Nodes
            assign recommended_browse_nodes = 7141124011
            case feed_product_type
                when 'boot'
                    case target_gender
                        when 'Female'
                            assign recommended_browse_nodes = 679380011
                        when 'Male'
                            assign recommended_browse_nodes = 5658904011
                    endcase
                when 'slipper'
                    case target_gender
                        when 'Female'
                            assign recommended_browse_nodes = 679433011
                        when 'Male'
                            assign recommended_browse_nodes = 679324011
                    endcase
                when 'shoe'
                    case target_gender
                        when 'Female'
                            assign recommended_browse_nodes = 679337011
                        when 'Male'
                            assign recommended_browse_nodes = 679313011
                endcase
            endcase

            assign parent_child = ''
            assign parent_sku = ''
            assign relationship_type = 'Variation'
            assign variation_theme = ''

            assign model_number = variant.sku
            assign outer_material_type = 'leather'

            if variant.image
                assign main_image_url = variant.image | image_url: width: 2000 | prepend: "https:"
            else
                assign main_image_url = product.featured_image | image_url: width: 2000 | prepend: "https:"
            endif

            # Image ALT text must exactly match variant color for images to be matched
            assign variantOtherImages = product.images | where: "alt", color_name | sort: 'attached_to_variant' | reverse

            assign other_image_url1 = ''
            assign other_image_url2 = ''
            assign other_image_url3 = ''
            assign other_image_url4 = ''
            assign other_image_url5 = ''
            assign other_image_url6 = ''
            assign other_image_url7 = ''
            assign other_image_url8 = ''

            if variantOtherImages[1]
                assign other_image_url1 = variantOtherImages[1] | image_url: width: 2000 | prepend: "https:"
            endif
            if variantOtherImages[2]
                assign other_image_url2 = variantOtherImages[2] | image_url: width: 2000 | prepend: "https:"
            endif
            if variantOtherImages[3]
                assign other_image_url3 = variantOtherImages[3] | image_url: width: 2000 | prepend: "https:"
            endif
            if variantOtherImages[4]
                assign other_image_url4 = variantOtherImages[4] | image_url: width: 2000 | prepend: "https:"
            endif
            if variantOtherImages[5]
                assign other_image_url5 = variantOtherImages[5] | image_url: width: 2000 | prepend: "https:"
            endif
            if variantOtherImages[6]
                assign other_image_url6 = variantOtherImages[6] | image_url: width: 2000 | prepend: "https:"
            endif
            if variantOtherImages[7]
                assign other_image_url7 = variantOtherImages[7] | image_url: width: 2000 | prepend: "https:"
            endif
            if variantOtherImages[8]
                assign other_image_url8 = variantOtherImages[8] | image_url: width: 2000 | prepend: "https:"
            endif
            -%}
