        assign outer_material_type = ''


        # Product Type, Item Type, Item Type Name
        assign item_type = 'Shoes'
        assign item_type_name = ''
        assign feed_product_type = 'none'
        case product.type
            when 'Mukluk'
                assign feed_product_type = 'boot'
            when 'Slipper'
                assign feed_product_type = 'slipper'
                assign item_type = 'slipper'
                assign item_type_name = 'slipper'
