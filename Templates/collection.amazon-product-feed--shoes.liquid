        {%- liquid
        assign model_name = product.title | replace: ',','' | remove: '"' | remove: "'" | remove: "," |  strip_html | strip

        # Item Name
        assign item_name = model_name
        unless item_name contains product.type
            assign item_name = item_name | append: ' ' | append: product.type
        endunless
        case model_name
        when 'Tipi'
            assign item_name = item_name | replace: 'Moccasin', 'Slipper'
        endcase

        # Description
        assign product_description = product.description | strip_html | strip_newlines | escape | remove: "," | truncate: 2000
